{
  "name": "chart-contest",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack-dev-server --mode development --hot",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --fix src/",
    "test": "jest --coverage",
    "check": "npm run lint && npm run test && npm run test:loki",
    "check-types": "tsc -p tsconfig.json --noEmit",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook --no-dll",
    "test:loki": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "stryker": "stryker run",
    "loki:update": "npm run build-storybook && npx loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "npx loki approve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsioman/chart.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tsioman/chart/issues"
  },
  "homepage": "https://github.com/tsioman/chart#readme",
  "devDependencies": {
